/*-- Position --*/
.right{text-align:right;}
.center{text-align:center;}
.left{text-align:left;}

/* TAB */
#TabAccount,#TabDiary,#TabDate,#TabFriends,#TabMess{height:25px;text-align:left;border-bottom:1px solid #3b5998;margin:0;padding:0;}
h2.maintitle{border:none;padding:0 0 0 25px;font-size:14px;color:#f9fcfd;font-weight:bold;line-height:30px;background:#5c75aa;margin:0;}

#TabAccount ul,
#TabDiary ul,
#TabDate ul,
#TabFriends ul,
#TabMess ul
	{line-height:20px;height:20px;list-style:none;padding:0;margin-left:5px;}
#TabAccount li .active,
#TabDiary li .active,
#TabDate li .active,
#TabFriends li .active,
#TabMess li .active
	{font-size:12px;text-align:center;float:left;background:#3b5998;/*background:url(/images/tabaction.png) no-repeat bottom center;width:90px;height:20px;line-height:20px;*/}
#TabAccount li.active a,#TabAccount li.active a:visited,
#TabDiary li.active a,#TabDiary li.active a:visited,
#TabDate li.active a,#TabDate li.active a:visited,
#TabFriends li.active a,#TabFriends li.active a:visited,
#TabMess li.active a,#TabMess li.active a:visited
	{color:#fff;text-decoration:none !important;padding:2px 10px;display:block;border-top:1px solid #333;border-right:1px solid #333;border-left:1px solid #333;}
#TabAccount li,
#TabDiary li,
#TabDate li,
#TabFriends li,
#TabMess li
	{margin-right:1px;/*background:url(/images/tab_normal.png) no-repeat top center;*/font-size:12px;text-align:center;float:left;background:#f0f0f0;/*width:90px;height:20px;line-height:20px;*/}
#TabAccount li a,#TabAccount li a:visited,
#TabDiary li a,#TabDiary li a:visited,
#TabDate li a,#TabDate li a:visited,
#TabFriends li a,#TabFriends li a:visited,
#TabMess li a,#TabMess li a:visited
	{color:#333;text-decoration:none !important;border-top:1px solid #898989;border-right:1px solid #898989;border-left:1px solid #898989;padding:2px 10px;display:block;}
	
